{
    "name": "pytheas-ui-wc",
    "version": "0.1.0",
    "scripts": {
        "BUILD-EXTERNAL-DEPENDENCIES": "",
        "build-tsquery": "npx browserify node_modules/@phenomnomnominal/tsquery/dist/src/index.js -s tsquery -o output/scripts/tsquery.js",
        "build-tsquery:prepare-analyse": "npx browserify node_modules/@phenomnomnominal/tsquery/dist/src/index.js -s tsquery --debug | exorcist output/scripts/tsquery.js.map > output/scripts/tsquery.js",
        "build-tsquery:analyse": "npx source-map-explorer --html output/scripts/tsquery.js output/scripts/tsquery.js.map > output/tsquery.html",
        "build-tsquery:prod": "npm run build-tsquery && npx google-closure-compiler --warning_level=QUIET --js=output/scripts/tsquery.js --js_output_file=output/scripts/tsquery.min.js && mv output/scripts/tsquery.min.js output/scripts/tsquery.js",
        "build-java-ast": "npx browserify node_modules/java-ast/dist/index.js -s javaast -o output/scripts/javaast.js",
        "build-java-ast:prod": "npx browserify node_modules/java-ast/dist/index.js -s javaast | uglifyjs -c > output/scripts/javaast.js",
        "build-browser-dependencies": "run-s build-tsquery build-java-ast",
        "build-browser-dependencies:prod": "run-s build-tsquery:prod build-java-ast:prod",
        "build-codemirror": "npx concat -o output/scripts/codemirror.js ./node_modules/codemirror/lib/codemirror.js ./node_modules/codemirror/mode/javascript/javascript.js ./node_modules/codemirror/addon/fold/foldcode.js ./node_modules/codemirror/addon/fold/foldgutter.js ./node_modules/codemirror/addon/fold/brace-fold.js",
        "postbuild-codemirror": "npx google-closure-compiler --warning_level=QUIET --js=output/scripts/codemirror.js --js_output_file=output/scripts/codemirror.min.js && mv output/scripts/codemirror.min.js output/scripts/codemirror.js",
        "compile-dragmock": "npx rollup -c test/vendors/drag-mock/rollup.config.js",
        "concat-app-dragmock": "npx concat -o output/scripts/app.js output/scripts/app.js output/scripts/dragmock.js test/mock-drag.js",
        "SASS": "",
        "compile-scss": "npx node-sass app/styles/app.scss output/styles/app.css",
        "TYPESCRIPT": "",
        "compile-ts": "NODE_ENV=development npx rollup -c rollup-app.config.js",
        "postcompile-ts": "npm run build-codemirror && npx filesize-cli output/scripts && npm run workbox",
        "compile-ts:prod": "NODE_ENV=production npx rollup -c rollup-app.config.js",
        "postcompile-ts:prod": "npm run build-codemirror",
        "MINIFICATION": "",
        "minify-js": "npx uglifyjs output/scripts/app.js --output output/scripts/app.js && npx uglifyjs output/scripts/codemirror.js --output output/scripts/codemirror.js",
        "minify-css": "npx uglifycss output/styles/app.css --output output/styles/app.css",
        "COPIES": "",
        "copy-all": "run-s copy-index copy-ionicons copy-assets",
        "copy-index": "npx copyfiles -u 1 -a app/index.html app/manifest.json output/",
        "copy-assets": "npx rimraf output/assets && mkdir output/assets && cp -R app/assets/* output/assets",
        "copy-ionicons": "npx rimraf output/libs && mkdir output/libs && mkdir output/libs/ionicons && cp -R node_modules/ionicons/dist/* output/libs/ionicons",
        "WEB-COMPONENTS": "",
        "build-wc": "stencil build --config ./components/stencil.config.ts && npm run copy-wc",
        "copy-wc": "npx rimraf output/web-components && mkdir output/web-components && cp -R ./components/dist output/web-components",
        "start-wc": "stencil build --dev --watch --serve --es5 --config ./components/stencil.config.ts",
        "PRODUCTION": "",
        "prebuild": "npx rimraf output",
        "build": "run-s compile-ts:prod build-wc copy-wc copy-all minify-js compile-scss minify-css build-browser-dependencies:prod workbox",
        "PWA": "",
        "workbox": "npx workbox generateSW workbox-config.js",
        "postworkbox": "npx concat -o output/sw.js output/sw.js app/sw.js",
        "DEVELOPMENT": "",
        "prestart": "npx rimraf output",
        "start": "npm-run-all -p serve watch build-browser-dependencies",
        "serve": "superstatic output --port 8383 --host localhost",
        "watch": "npm-watch",
        "DOCUMENTATION": "",
        "doc:code": "npx typedoc --out documentation/code-documentation --module commonjs --ignoreCompilerErrors app",
        "doc:contributor:serve": "npx gitbook serve documentation/contributor-documentation",
        "doc:contributor:build": "npx gitbook build documentation/contributor-documentation",
        "madge": "npx madge --warning -i doc/dependency-graph.png app/**/*.ts",
        "CODEQUALITY": "",
        "lint": "npx tslint -p tsconfig.json -c tslint.json",
        "source-explore": "npx source-map-explorer --html output/scripts/app.js > output/tree.html",
        "TESTS": "",
        "build:dev:test": "npm run compile-dragmock && npm run build-wc && npm run compile-ts && npm run concat-app-dragmock && npm run copy-all && npm run compile-scss && npm run build-browser-dependencies",
        "test:e2e": "run-p --race serve test:testcafe:remote",
        "test:e2e:local": "run-p --race serve test:testcafe:local",
        "test:e2e:saucelabs": "run-p --race serve test:testcafe:saucelabs",
        "test:testcafe:local": "npx testcafe chromium test/testcafe/test.js",
        "test:testcafe:remote": "npx testcafe chrome,firefox test/testcafe/test.js",
        "test:testcafe:saucelabs": "npx testcafe \"saucelabs:Chrome@48.0:Linux\" test/testcafe/test.js --app \"http-server output -p 8383\"",
        "test:unit": "jest --config jest.config.js --coverage",
        "test:wc": "jest --config jest.wc.config.js --coverage"
    },
    "watch": {
        "build-wc": {
            "patterns": [
                "components"
            ],
            "extensions": "tsx"
        },
        "compile-ts": {
            "patterns": [
                "app"
            ],
            "extensions": "ts"
        },
        "copy-all": {
            "patterns": [
                "app"
            ],
            "extensions": "html"
        },
        "compile-scss": {
            "patterns": [
                "app"
            ],
            "extensions": "scss"
        }
    },
    "dependencies": {
        "@phenomnomnominal/tsquery": "^2.1.1",
        "codemirror": "^5.41.0",
        "dayjs": "^1.7.7",
        "dom-to-image": "^2.6.0",
        "downloadjs": "^1.4.7",
        "ionicons": "^4.4.6",
        "java-ast": "^0.1.0",
        "jsplumb": "^2.8.3",
        "notifier-js": "^1.0.0",
        "panzoom": "^6.3.2",
        "sloc": "^0.2.0",
        "split.js": "^1.5.7",
        "workbox": "0.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.1.2",
        "@stencil/core": "0.15.0",
        "@stencil/sass": "^0.1.1",
        "@types/filesystem": "0.0.29",
        "@types/jest": "^23.3.8",
        "@types/split.js": "^1.3.1",
        "babel-preset-es2015-rollup": "^3.0.0",
        "browserify": "^16.2.3",
        "concat": "^1.0.3",
        "copyfiles": "^2.1.0",
        "exorcist": "^1.0.1",
        "filesize-cli": "0.0.3",
        "gitbook-cli": "^2.3.2",
        "google-closure-compiler": "^20181008.0.0",
        "jest": "^23.6.0",
        "madge": "^3.2.0",
        "node-sass": "^4.9.4",
        "nodemon": "^1.18.5",
        "npm-run-all": "^4.1.3",
        "npm-watch": "^0.4.0",
        "rimraf": "^2.6.2",
        "rollup": "^0.66.6",
        "rollup-plugin-babel": "^4.0.3",
        "rollup-plugin-commonjs": "^9.2.0",
        "rollup-plugin-node-builtins": "^2.1.2",
        "rollup-plugin-node-globals": "^1.4.0",
        "rollup-plugin-node-resolve": "^3.4.0",
        "rollup-plugin-typescript": "^1.0.0",
        "source-map-explorer": "^1.6.0",
        "superstatic": "^6.0.3",
        "testcafe": "^0.23.0",
        "testcafe-browser-provider-saucelabs": "^1.3.1",
        "ts-jest": "^23.10.4",
        "tslib": "^1.9.3",
        "tslint": "^5.11.0",
        "typedoc": "^0.13.0",
        "typescript": "^3.1.3",
        "uglify-es": "3.3.9",
        "uglifycss": "0.0.29",
        "workbox-cli": "^3.6.3"
    }
}
