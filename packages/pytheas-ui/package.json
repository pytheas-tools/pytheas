{
    "name": "pytheas-ui",
    "version": "0.1.0",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pytheas-tools/pytheas.git"
    },
    "author": "Vincent Ogloblinsky",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/pytheas-tools/pytheas/issues"
    },
    "homepage": "https://github.com/pytheas-tools/pytheas#readme",
    "scripts": {
        "---DEVELOPMENT---": "",
        "prestart": "npm run clean-output && mkdir output && mkdir output/scripts",
        "start": "npm-run-all -p serve watch build-browser-dependencies copy-demos",
        "serve": "live-server output --port=8383 --host=localhost --quiet",
        "watch": "npm-watch",
        "---BUILD-EXTERNAL-DEPENDENCIES---": "",
        "build-browser-dependencies": "cp -R ../pytheas-ui-static-dependencies/output/* output/scripts",
        "build-browser-dependencies:prod": "cd ../pytheas-ui-static-dependencies && npm run build:prod && cd ../pytheas-ui && npm run build-browser-dependencies",
        "compile-dragmock": "npx rollup -c test/vendors/drag-mock/rollup.config.js",
        "concat-app-dragmock": "npx concat -o output/scripts/app_es6.js output/scripts/app_es6.js output/scripts/dragmock.js test/mock-drag.js",
        "---SASS---": "",
        "compile-scss": "npx node-sass src/styles/app.scss output/styles/app.css",
        "---TYPESCRIPT---": "",
        "compile-ts": "NODE_ENV=development npx rollup -c rollup-app.config.js",
        "compile-ts:prod": "NODE_ENV=production npx rollup -c rollup-app.config.js",
        "postcompile-ts:prod": "run-s clean-prod-file clean-prod-file-es6",
        "clean-prod-file": "npx strip-debug output/scripts/app_0.js > output/scripts/app.js && rm output/scripts/app_0.js",
        "clean-prod-file-es6": "npx strip-debug output/scripts/app_es6_0.js > output/scripts/app_es6.js && rm output/scripts/app_es6_0.js",
        "---MINIFICATION---": "",
        "minify-js": "npx uglifyjs output/scripts/app.js --output output/scripts/app.js && npx uglifyjs output/scripts/app_es6.js --output output/scripts/app_es6.js",
        "minify-css": "npx uglifycss output/styles/app.css --output output/styles/app.css",
        "---COPIES---": "",
        "copy-all": "run-s copy-index replace-ionicons copy-assets",
        "copy-index": "npx copyfiles -u 1 -a src/index.html src/manifest.json src/favicon.ico output/",
        "postcopy-index": "node scripts/update-version-during-build.js",
        "copy-assets": "npx rimraf output/assets && mkdir output/assets && cp -R src/assets/* output/assets",
        "replace-ionicons": "replace \"libs/ionicons/ionicons.js\" \"https://unpkg.com/ionicons@4.4.4/dist/ionicons.js\" output/index.html output/index.html",
        "---WEB-COMPONENTS---": "",
        "build-wc": "stencil build --config ./components/stencil.config.ts && npm run copy-wc",
        "build-wc-prod": "NODE_ENV=production stencil build --config ./components/stencil.config.ts && npm run copy-wc",
        "copy-wc": "npx rimraf output/web-components && mkdir output/web-components && cp -R ./components/dist output/web-components",
        "start-wc": "stencil build --dev --watch --serve --es5 --config ./components/stencil.config.ts",
        "---BUILD-UTILS--": "",
        "clean-output": "npx rimraf output",
        "update-context:vscode": "replace \"__PYTHEAS_CONTEXT__\" \"vscode\" output/index.html output/index.html",
        "update-context:browser": "replace \"__PYTHEAS_CONTEXT__\" \"web\" output/index.html output/index.html",
        "update-context:electron": "replace \"__PYTHEAS_CONTEXT__\" \"electron\" output/index.html output/index.html",
        "---BUILD-TEST---": "",
        "build:dev:test": "run-s compile-dragmock build-wc compile-ts concat-app-dragmock copy-all compile-scss build-browser-dependencies update-context:browser",
        "---BUILD-DEV--": "",
        "prebuild-dev": "npm run clean-output",
        "build-dev": "run-s compile-ts build-wc copy-wc copy-all copy-demos compile-scss build-browser-dependencies",
        "---BUILD-DEV-VSCODE--": "",
        "prebuild-dev:vscode": "npm run clean-output",
        "build-dev:vscode": "run-s compile-ts build-wc copy-wc copy-all compile-scss build-browser-dependencies update-context:vscode",
        "---BUILD-PROD--": "",
        "prebuild": "npm run clean-output",
        "build": "run-s compile-ts:prod build-wc-prod copy-wc copy-all copy-demos minify-js compile-scss minify-css build-browser-dependencies:prod update-context:browser workbox",
        "---BUILD-VSCODE--": "",
        "prebuild:vcode": "npm run clean-output",
        "build:vscode": "run-s compile-ts:prod build-wc-prod copy-wc copy-all copy-demos minify-js compile-scss minify-css build-browser-dependencies:prod update-context:vscode",
        "copy-for-vscode": "npx rimraf ../pytheas-vscode/media && mkdir ../pytheas-vscode/media && cp -R output/ ../pytheas-vscode/media",
        "---BUILD-ELECTRON--": "",
        "prebuild:electron": "npm run clean-output",
        "build:electron": "",
        "---PWA---": "",
        "workbox": "npx workbox generateSW workbox-config.js",
        "postworkbox": "npx concat -o output/sw.js output/sw.js src/sw.js",
        "---DEMOS---": "",
        "precopy-demos": "mkdir output/demos && mkdir output/demos/typescript && mkdir output/demos/javascript",
        "copy-demos": "run-p copy-demos-typescript copy-demos-javascript",
        "postcopy-demos": "node scripts/list-demo-files.js",
        "copy-demos-typescript": "cp -r ../../test-sources/typescript/ output/demos/typescript",
        "copy-demos-javascript": "cp -r ../../test-sources/es6/ output/demos/javascript",
        "---DOCUMENTATION---": "",
        "doc:code": "npx typedoc --out code-documentation --mode file --module commonjs --ignoreCompilerErrors src",
        "doc:code:serve": "live-server code-documentation",
        "---CODEQUALITY---": "",
        "lint": "npx tslint -p tsconfig.json -c tslint.json 'src/**/*.ts'",
        "source-explore": "npx source-map-explorer --html output/scripts/app.js > output/tree.html",
        "---TESTS---": "",
        "test:e2e": "run-p --race serve test:testcafe:remote",
        "test:e2e:local": "run-p --race serve test:testcafe:local",
        "test:e2e:saucelabs": "run-p --race serve test:testcafe:saucelabs",
        "test:testcafe:local": "npx testcafe chromium test/testcafe/test.js",
        "test:testcafe:remote": "npx testcafe chrome,firefox test/testcafe/test.js",
        "test:testcafe:saucelabs": "npx testcafe \"saucelabs:Chrome@48.0:Linux\" test/testcafe/test.js --src \"http-server output -p 8383\"",
        "test:unit": "jest --config jest.config.js --coverage",
        "test:wc": "jest --config jest.wc.config.js --coverage"
    },
    "watch": {
        "build-wc": {
            "patterns": [
                "components"
            ],
            "extensions": "tsx"
        },
        "compile-ts": {
            "patterns": [
                "src"
            ],
            "extensions": "ts"
        },
        "copy-all": {
            "patterns": [
                "src"
            ],
            "extensions": "html"
        },
        "compile-scss": {
            "patterns": [
                "src"
            ],
            "extensions": "scss"
        }
    },
    "dependencies": {
        "@phenomnomnominal/tsquery": "^3.0.0",
        "codemirror": "^5.46.0",
        "dayjs": "^1.8.14",
        "dom-to-image": "^2.6.0",
        "downloadjs": "^1.4.7",
        "ionicons": "^4.5.8",
        "notifier-js": "^1.0.0",
        "panzoom": "^8.1.0",
        "sloc": "^0.2.1",
        "split.js": "^1.5.10"
    },
    "devDependencies": {
        "@babel/core": "^7.4.4",
        "@stencil/core": "0.18.1",
        "@stencil/sass": "^0.2.1",
        "@types/filesystem": "0.0.29",
        "@types/jest": "^24.0.12",
        "@types/split.js": "^1.3.2",
        "babel-preset-es2015-rollup": "^3.0.0",
        "concat": "^1.0.3",
        "copyfiles": "^2.1.0",
        "exorcist": "^1.0.1",
        "filesize-cli": "0.0.3",
        "fs-extra": "^8.0.0",
        "google-closure-compiler": "^20190415.0.0",
        "jest": "^24.8.0",
        "live-server": "^1.2.1",
        "nodemon": "^1.19.0",
        "npm-run-all": "^4.1.5",
        "npm-watch": "^0.6.0",
        "recursive-readdir": "^2.2.2",
        "replace": "^1.1.0",
        "rimraf": "^2.6.3",
        "rollup": "^1.11.3",
        "rollup-plugin-babel": "^4.3.2",
        "rollup-plugin-commonjs": "^9.3.4",
        "rollup-plugin-node-builtins": "^2.1.2",
        "rollup-plugin-node-globals": "^1.4.0",
        "rollup-plugin-node-resolve": "4.2.4",
        "rollup-plugin-typescript": "^1.0.1",
        "source-map-explorer": "^1.8.0",
        "stencil-strip-debug": "^1.0.0",
        "strip-debug-cli": "^2.0.0",
        "testcafe": "^1.1.4",
        "testcafe-browser-provider-saucelabs": "^1.7.0",
        "ts-jest": "^24.0.2",
        "tslib": "^1.9.3",
        "tslint": "^5.16.0",
        "typedoc": "^0.14.2",
        "typescript": "^3.4.5",
        "uglify-es": "3.3.9",
        "uglifycss": "0.0.29",
        "useref": "^1.4.1",
        "workbox-cli": "^4.3.1"
    }
}
