{
    "name": "pytheas-ui-wc",
    "version": "0.1.0",
    "scripts": {
        "BUILD-EXTERNAL-DEPENDENCIES": "",
        "build-tsquery": "npx browserify node_modules/@phenomnomnominal/tsquery/dist/src/index.js -s tsquery -o .tmp/scripts/tsquery.js",
        "build-tsquery:prod": "npx browserify node_modules/@phenomnomnominal/tsquery/dist/src/index.js -s tsquery | uglifyjs -c > dist/scripts/tsquery.js",
        "build-java-ast": "npx browserify node_modules/java-ast/dist/index.js -s javaast -o .tmp/scripts/javaast.js",
        "build-java-ast:prod": "npx browserify node_modules/java-ast/dist/index.js -s javaast | uglifyjs -c > dist/scripts/javaast.js",
        "build-browser-dependencies": "run-s build-tsquery build-java-ast",
        "build-browser-dependencies:prod": "run-s build-tsquery:prod build-java-ast:prod",
        "concat-wc-codemirror": "npx concat -o .tmp/scripts/codemirror.js ./node_modules/codemirror/lib/codemirror.js ./node_modules/codemirror/mode/javascript/javascript.js ./node_modules/codemirror/addon/fold/foldcode.js ./node_modules/codemirror/addon/fold/foldgutter.js ./node_modules/codemirror/addon/fold/brace-fold.js",
        "concat-wc-codemirror:prod": "npx concat -o dist/scripts/codemirror.js ./node_modules/codemirror/lib/codemirror.js ./node_modules/codemirror/mode/javascript/javascript.js ./node_modules/codemirror/addon/fold/foldcode.js ./node_modules/codemirror/addon/fold/foldgutter.js ./node_modules/codemirror/addon/fold/brace-fold.js",
        "compile-dragmock": "npx rollup -c test/vendors/drag-mock/rollup.config.js",
        "concat-app-dragmock": "npx concat -o .tmp/scripts/app.js .tmp/scripts/app.js .tmp/scripts/dragmock.js test/mock-drag.js",
        "SASS": "",
        "compile-scss": "npx node-sass app/styles/app.scss .tmp/styles/app.css",
        "compile-scss:prod": "npx node-sass app/styles/app.scss dist/styles/app.css",
        "postcompile-scss:prod": "npm run minify-css",
        "TYPESCRIPT": "",
        "compile-ts": "npx rollup -c rollup-app_dev.config.js",
        "postcompile-ts": "npm run concat-wc-codemirror && npx filesize-cli .tmp/scripts && npm run workbox",
        "compile-ts:prod": "npx rollup -c rollup-app_prod.config.js",
        "postcompile-ts:prod": "npm run concat-wc-codemirror:prod && npx filesize-cli dist/scripts",
        "MINIFICATION": "",
        "minify-js": "npx uglifyjs dist/scripts/app.js --output dist/scripts/app.js && npx uglifyjs dist/scripts/codemirror.js --output dist/scripts/codemirror.js",
        "minify-css": "npx uglifycss dist/styles/app.css --output dist/styles/app.css",
        "COPIES": "",
        "copy-all": "run-s copy-index copy-ionicons copy-assets",
        "copy-all:prod": "run-s copy-index:prod copy-ionicons:prod copy-assets:prod",
        "copy-index": "npx copyfiles -u 1 -a app/index.html app/manifest.json .tmp/",
        "copy-index:prod": "npx copyfiles -u 1 app/index.html app/manifest.json app/assets/** dist/",
        "copy-assets": "npx rimraf .tmp/assets && mkdir .tmp/assets && cp -R app/assets/* .tmp/assets",
        "copy-assets:prod": "npx rimraf dist/assets && mkdir dist/assets && cp -R app/assets/* dist/assets",
        "copy-ionicons": "npx rimraf .tmp/libs && mkdir .tmp/libs && mkdir .tmp/libs/ionicons && cp -R node_modules/ionicons/dist/* .tmp/libs/ionicons",
        "copy-ionicons:prod": "npx rimraf dist/libs && mkdir dist/libs && mkdir dist/libs/ionicons && cp -R node_modules/ionicons/dist/* dist/libs/ionicons",
        "WEB-COMPONENTS": "",
        "build-wc": "stencil build --config ./components/stencil.config.ts && npm run copy-wc",
        "build-wc:prod": "stencil build --config ./components/stencil.config.ts && npm run copy-wc:prod",
        "copy-wc": "npx rimraf .tmp/web-components && mkdir .tmp/web-components && cp -R ./components/dist .tmp/web-components",
        "copy-wc:prod": "npx rimraf dist/web-components && mkdir dist/web-components && cp -R ./components/dist dist/web-components",
        "start-wc": "stencil build --dev --watch --serve --es5 --config ./components/stencil.config.ts",
        "PRODUCTION": "",
        "prebuild:prod": "npx rimraf dist",
        "build:prod": "run-s compile-ts:prod build-wc:prod copy-wc:prod copy-all:prod minify-js compile-scss:prod build-browser-dependencies:prod workbox:prod",
        "PWA": "",
        "workbox": "npx workbox generateSW workbox-config.js",
        "workbox:prod": "npx workbox generateSW workbox-config_prod.js",
        "postworkbox": "npx concat -o .tmp/sw.js .tmp/sw.js app/sw.js",
        "postworkbox:prod": "npx concat -o dist/sw.js dist/sw.js app/sw.js",
        "DEVELOPMENT": "",
        "prestart": "npx rimraf .tmp",
        "start": "npm-run-all -p serve watch build-browser-dependencies",
        "serve": "superstatic .tmp --port 8383 --host localhost",
        "watch": "npm-watch",
        "DOCUMENTATION": "",
        "doc:code": "npx typedoc --out documentation/code-documentation --module commonjs --ignoreCompilerErrors app",
        "doc:contributor:serve": "npx gitbook serve documentation/contributor-documentation",
        "doc:contributor:build": "npx gitbook build documentation/contributor-documentation",
        "madge": "npx madge --warning -i doc/dependency-graph.png app/**/*.ts",
        "CODEQUALITY": "",
        "lint": "npx tslint -p tsconfig.json -c tslint.json",
        "source-explore": "npx source-map-explorer --html .tmp/scripts/app.js > .tmp/tree.html",
        "TESTS": "",
        "build:dev:test": "npm run compile-dragmock && npm run build-wc && npm run compile-ts && npm run concat-app-dragmock && npm run copy-all && npm run compile-scss && npm run build-browser-dependencies",
        "test:e2e": "run-p --race serve test:testcafe:remote",
        "test:e2e:local": "run-p --race serve test:testcafe:local",
        "test:e2e:saucelabs": "run-p --race serve test:testcafe:saucelabs",
        "test:testcafe:local": "npx testcafe chromium test/testcafe/test.js",
        "test:testcafe:remote": "npx testcafe chrome,firefox test/testcafe/test.js",
        "test:testcafe:saucelabs": "npx testcafe \"saucelabs:Chrome@48.0:Linux\" test/testcafe/test.js --app \"http-server .tmp -p 8383\"",
        "test:unit": "jest --config jest.config.js --coverage",
        "test:wc": "jest --config jest.wc.config.js --coverage"
    },
    "watch": {
        "build-wc": {
            "patterns": [
                "components"
            ],
            "extensions": "tsx"
        },
        "compile-ts": {
            "patterns": [
                "app"
            ],
            "extensions": "ts"
        },
        "copy-all": {
            "patterns": [
                "app"
            ],
            "extensions": "html"
        },
        "compile-scss": {
            "patterns": [
                "app"
            ],
            "extensions": "scss"
        }
    },
    "dependencies": {
        "@phenomnomnominal/tsquery": "^2.1.1",
        "codemirror": "^5.41.0",
        "dayjs": "^1.7.7",
        "dom-to-image": "^2.6.0",
        "ionicons": "^4.4.6",
        "java-ast": "^0.1.0",
        "jsplumb": "^2.8.2",
        "notifier-js": "^1.0.0",
        "sloc": "^0.2.0",
        "split.js": "^1.5.7",
        "workbox": "0.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.1.2",
        "@stencil/core": "0.15.0",
        "@stencil/sass": "^0.1.1",
        "@types/filesystem": "0.0.29",
        "@types/jest": "^23.3.7",
        "@types/split.js": "^1.3.1",
        "babel-preset-es2015-rollup": "^3.0.0",
        "browserify": "^16.2.3",
        "concat": "^1.0.3",
        "copyfiles": "^2.1.0",
        "filesize-cli": "0.0.3",
        "gitbook-cli": "^2.3.2",
        "jest": "^23.6.0",
        "madge": "^3.2.0",
        "node-sass": "^4.9.4",
        "nodemon": "^1.18.5",
        "npm-run-all": "^4.1.3",
        "npm-watch": "^0.4.0",
        "rimraf": "^2.6.2",
        "rollup": "^0.66.6",
        "rollup-plugin-babel": "^4.0.3",
        "rollup-plugin-commonjs": "^9.2.0",
        "rollup-plugin-node-builtins": "^2.1.2",
        "rollup-plugin-node-globals": "^1.4.0",
        "rollup-plugin-node-resolve": "^3.4.0",
        "rollup-plugin-typescript": "^1.0.0",
        "source-map-explorer": "^1.6.0",
        "superstatic": "^6.0.3",
        "testcafe": "^0.23.0",
        "testcafe-browser-provider-saucelabs": "^1.3.1",
        "ts-jest": "^23.10.4",
        "tslib": "^1.9.3",
        "tslint": "^5.11.0",
        "typedoc": "^0.13.0",
        "typescript": "^3.1.3",
        "uglify-es": "3.3.9",
        "uglifycss": "0.0.29",
        "workbox-cli": "^3.6.3"
    }
}